	  $(MKDIR_P) "$(DESTDIR)$(man3_setsdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(man3_setsdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(man3_setsdir)" || exit $$?; \
	done

